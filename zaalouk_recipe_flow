// Zaalouk Recipe Flow
digraph {
	rankdir=TB
	subgraph cluster_ingredients {
		node [fontname=Arial shape=rect style=filled]
		color=lightgrey fontname=Arial label=Ingredients style=filled
		Eggplants [label="2 Large Eggplants" color=purple]
		Tomatoes [label="2 Medium/Large Tomatoes" color=red]
		Garlic [label="6 Cloves Garlic" color=lightyellow]
		Parsley [label="2 Tbsp Fresh Parsley" color=green]
		Cilantro [label="2 Tbsp Fresh Cilantro" color=green]
		Spices [label="1 tsp Salt
2 tsp Paprika
2 tsp Cumin
Cayenne Pepper (optional)" color=orange]
		"Olive Oil" [label="4 Tbsp Olive Oil" color=yellow]
		Water [label="1/4 - 1/3 Cup Water" color=lightblue]
		"Lemon Juice" [label="Lemon Juice (optional)" color=lightyellow]
	}
	subgraph cluster_prep {
		node [fontname=Arial style=filled]
		edge [fontname=Arial]
		color=lightgrey fontname=Arial label=Preparation style=filled
		"Chop Tomatoes" [label="Chop Tomatoes" color=red shape=parallelogram]
		Tomatoes -> "Chop Tomatoes"
		"Press Garlic" [label="Press Garlic" color=lightyellow shape=parallelogram]
		Garlic -> "Press Garlic"
		"Chop Parsley" [label="Chop Parsley" color=green shape=parallelogram]
		Parsley -> "Chop Parsley"
		"Chop Cilantro" [label="Chop Cilantro" color=green shape=parallelogram]
		Cilantro -> "Chop Cilantro"
		"Peel and Chop Eggplants" [label="Peel and Chop Eggplants" color=purple shape=parallelogram]
		Eggplants -> "Peel and Chop Eggplants"
	}
	subgraph cluster_cook {
		node [color=white fontname=Arial style=filled]
		edge [fontname=Arial]
		color=lightgrey fontname=Arial label=Cooking style=filled
		"Combine Initial" [label="Combine in Skillet/Pot" shape=diamond]
		"Chop Tomatoes" -> "Combine Initial" [label="add to"]
		"Press Garlic" -> "Combine Initial" [label="add to"]
		"Chop Parsley" -> "Combine Initial" [label="add to"]
		"Chop Cilantro" -> "Combine Initial" [label="add to"]
		Spices -> "Combine Initial" [label="add to"]
		"Olive Oil" -> "Combine Initial" [label="add to"]
		"Add Eggplants" [label="Add Eggplants and Water"]
		"Combine Initial" -> "Add Eggplants" [label="add to"]
		"Peel and Chop Eggplants" -> "Add Eggplants" [label="add to"]
		Water -> "Add Eggplants" [label="add to"]
		"Cook 1" [label="Cook Covered (10-15 min)" color=orange shape=oval]
		"Add Eggplants" -> "Cook 1"
		Stir [label="Stir to Combine"]
		"Cook 1" -> Stir
		"Cook 2" [label="Cook Covered (15-20 min)" color=orange shape=oval]
		Stir -> "Cook 2"
		"Lemon Juice" -> "Cook 2" [label="add (optional)"]
		"Reduce Liquid" [label="Reduce Liquid, Stirring Frequently" color=orange shape=oval]
		"Cook 2" -> "Reduce Liquid"
		Mash [label="Mash (optional)" shape=diamond]
		"Reduce Liquid" -> Mash
		"Cook Final" [label="Continue Cooking to Desired Consistency" color=orange shape=oval]
		Mash -> "Cook Final"
		Zaalouk [label=Zaalouk color=brown shape=rectangle]
		"Cook Final" -> Zaalouk
	}
}
