		green_ingredients -> blend_green [arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
		salt2 -> blend_green [arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
		fry_green_salsa [label="Fry Green Salsa" fillcolor=tomato fontname=Helvetica fontsize=12 shape=oval style="rounded, filled"]
