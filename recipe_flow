		blend_red [label="Blend Until Smooth" fillcolor=lightsalmon fontname=Helvetica fontsize=12 shape=diamond style="rounded, filled"]
		drain_veg -> red_ingredients [label=take arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
		red_ingredients -> blend_red [arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
		salt1 -> blend_red [arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
		fry_red_salsa [label="Fry Red Salsa" fillcolor=tomato fontname=Helvetica fontsize=12 shape=oval style="rounded, filled"]
		olive_oil1 [label="1 tsp Olive Oil" fontname=Helvetica fontsize=12 shape=plaintext style=""]
		olive_oil1 -> fry_red_salsa [arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
		blend_red -> fry_red_salsa [label="1 min" arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
		red_salsa_bowl [label="Red Salsa Bowl" fillcolor=palegreen fontname=Helvetica fontsize=12 shape=doublecircle style="rounded, filled"]
		fry_red_salsa -> red_salsa_bowl [label="transfer to" arrowhead=normal arrowsize=0.7 fontname=Helvetica fontsize=10 penwidth=0.8 style=solid]
	}
