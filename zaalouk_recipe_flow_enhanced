// The Delightful Zaalouk Recipe
digraph {
	node [color=lightgoldenrodyellow fontname="Helvetica Neue" penwidth=1.5 shape=rect style="rounded,filled"]
	edge [arrowsize=0.8 color=steelblue fontname="Helvetica Neue Italic" fontsize=11 penwidth=1.2]
	bgcolor=cornsilk fontcolor=midnightblue fontname="Helvetica Neue" fontsize=16 label="The Delightful Zaalouk Recipe" labeljust=c labelloc=t rankdir=TB
	subgraph cluster_ingredients {
		color=palegoldenrod fontcolor=darkgreen fontname="Helvetica Neue Bold" fontsize=14 label="Fresh Ingredients" labeljust=l style=filled
		Eggplants [label="2 Large Eggplants
(Cubed)" color=mediumpurple3]
		Tomatoes [label="2 Medium/Large Tomatoes
(Diced)" color=tomato]
		Garlic [label="6 Cloves Garlic
(Minced/Pressed)" color=wheat2]
		Parsley [label="2 Tbsp Fresh Parsley
(Chopped)" color=seagreen3]
		Cilantro [label="2 Tbsp Fresh Cilantro
(Chopped)" color=seagreen3]
		Spices [label="1 tsp Salt
2 tsp Paprika
2 tsp Cumin
Cayenne Pepper (to taste)" color=sandybrown]
		"Olive Oil" [label="4 Tbsp Olive Oil" color=gold]
		Water [label="1/4 - 1/3 Cup Water" color=skyblue]
		"Lemon Juice" [label="Lemon Juice (to taste)" color=lemonchiffon2]
	}
	subgraph cluster_prep {
		color=lightcyan2 fontcolor=darkblue fontname="Helvetica Neue Bold" fontsize=14 label="Preparation Steps" labeljust=l style=filled
		"Chop Tomatoes" [label="Dice Tomatoes" color=tomato shape=parallelogram]
		Tomatoes -> "Chop Tomatoes"
		"Press Garlic" [label="Mince/Press Garlic" color=wheat2 shape=parallelogram]
		Garlic -> "Press Garlic"
		"Chop Parsley" [label="Chop Parsley" color=seagreen3 shape=parallelogram]
		Parsley -> "Chop Parsley"
		"Chop Cilantro" [label="Chop Cilantro" color=seagreen3 shape=parallelogram]
		Cilantro -> "Chop Cilantro"
		"Peel and Chop Eggplants" [label="Peel and Cube Eggplants" color=mediumpurple3 shape=parallelogram]
		Eggplants -> "Peel and Chop Eggplants"
	}
	subgraph cluster_cooking {
		color=peachpuff fontcolor=firebrick fontname="Helvetica Neue Bold" fontsize=14 label="Cooking Process" labeljust=l style=filled
		"Combine Initial" [label="Combine in Skillet" color=lightyellow shape=oval]
		"Chop Tomatoes" -> "Combine Initial" [label=add]
		"Press Garlic" -> "Combine Initial" [label=add]
		"Chop Parsley" -> "Combine Initial" [label=add]
		"Chop Cilantro" -> "Combine Initial" [label=add]
		Spices -> "Combine Initial" [label=add]
		"Olive Oil" -> "Combine Initial" [label=add]
		"Add Eggplants" [label="Add Eggplants
and Water" color=lightyellow shape=oval]
		"Combine Initial" -> "Add Eggplants" [label=then]
		"Peel and Chop Eggplants" -> "Add Eggplants" [label=add]
		Water -> "Add Eggplants" [label=add]
		"Cook 1" [label="Cook Covered
(10-15 min)" color=lightsalmon shape=oval]
		"Add Eggplants" -> "Cook 1"
		Stir [label="Stir Gently" color=lightyellow shape=oval]
		"Cook 1" -> Stir
		"Cook 2" [label="Cook Covered
(15-20 min)" color=lightsalmon shape=oval]
		Stir -> "Cook 2"
		"Lemon Juice" -> "Cook 2" [label=add]
		"Reduce Liquid" [label="Reduce Liquid
(Stir Frequently)" color=lightyellow shape=oval]
		"Cook 2" -> "Reduce Liquid"
		Mash [label="Mash to desired texture" color=lightyellow shape=oval]
		"Reduce Liquid" -> Mash
		"Cook Final" [label="Continue Cooking
to Desired Consistency" color=lightsalmon shape=oval]
		Mash -> "Cook Final"
		Zaalouk [label="Delicious Zaalouk!
(Serve Warm)" color=darkorange fontcolor=white shape=rectangle]
		"Cook Final" -> Zaalouk
	}
}
